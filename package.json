{
  "name": "@kyri123/express-app-router",
  "version": "1.0.9",
  "private": false,
  "author": "Oliver Kaufmann <kaufmann.oliver@siemens.com>",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "zod": "^3.22.2"
  },
  "scripts": {
    "create-docs": "typedoc --options typedoc.cjs",
    "dev": "npx nodemon",
    "build": "bun run build:lint && bun run build:rollup",
    "build:lint": "eslint --ext .ts ./src/ --fix",
    "build:rollup": "rollup -c --bundleConfigAsCjs",
    "test": "bun run build:rollup && jest --verbose",
    "testnb": "jest --verbose",
    "testw": "jest --watch --verbose",
    "prepare": "husky install",
    "hooks:pre-commit": "bun run create-docs && prettier --write . && git add .",
    "hooks:pre-push": "npm run build",
    "run-prettier": "npx prettier --write **/*.{js,ts,json,tsx,vue,cjs,mjs}"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.18",
    "@types/express-serve-static-core": "^4.17.37",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^18.18.1",
    "@types/semver": "^7.5.3",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rollup": "^3.29.4",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "semver": "^7.5.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.1.6"
  }
}
